{
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "build:prod": {
      "dependsOn": ["^build:prod"]
    },
    "dev-install": {
      "dependsOn": ["^dev-install"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test"]
      }
    }
  }
}
